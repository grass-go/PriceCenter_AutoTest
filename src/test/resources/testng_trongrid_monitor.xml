<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="trongridAPI" parallel="tests" thread-count="1">
    <listeners>
            <listener class-name="tron.common.utils.RetryListener"/>
    </listeners>
    <test name="trongridAPI-singapore">
        <parameter name="trongridUrl" value="https://api.singapore.trongrid.io/"/>
        <packages>
            <package name="tron.trongrid.trongridCase.*"></package>
        </packages>
<!--      <classes>
        <class name="tron.trongrid.trongridCase.V1API.transactionInfoByContractAddress"></class>
      </classes>-->
    </test>
    <test name="trongridAPI-oregon">
        <parameter name="trongridUrl" value="https://api.oregon.trongrid.io/"/>
                <packages>
                    <package name="tron.trongrid.trongridCase.*"></package>
                </packages>
    </test>
    <test name="trongridAPI-frankfurt">
        <parameter name="trongridUrl" value="https://api.frankfurt.trongrid.io/"/>
                <packages>
                    <package name="tron.trongrid.trongridCase.*"></package>
                </packages>
    </test>
    <test name="trongridAPI-v1event">
        <parameter name="trongridUrl" value="https://v1event.trongrid.io/"/>
        <packages>
            <package name="tron.trongrid.trongridCase.V1API.*"></package>
        </packages>
    </test>
</suite>